# Django secret key (use a placeholder value)
SECRET_KEY=your-secret-key

# Debug mode (set to True or False)
DEBUG=True

# Allowed hosts (list of hosts or IPs allowed to access your Django site)
ALLOWED_HOSTS=localhost,127.0.0.1

# PostgreSQL database configuration (use a placeholder value)
DATABASE_URL=postgres://user:password@localhost:5432/database

# Redis cache configuration (use a placeholder value)
CACHE_URL=redis://127.0.0.1:6379/1

# Email configuration (use placeholder values)
EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_HOST_USER=your-email@example.com
EMAIL_HOST_PASSWORD=your-email-password
EMAIL_USE_TLS=True
EMAIL_USE_SSL=False
